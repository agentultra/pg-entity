packages: ./

tests: True

documentation: True

test-show-details: direct

extra-lib-dirs:
  /usr/pgsql-14/lib

package pg-entity
  ghc-options:
    "-L /usr/pgsql-14/lib" 
  haddock-options:
    "--optghc=-L /usr/pgsql-14/lib" 
  -- flags: +prod -- uncomment this to deactivate stdout logging
allow-newer: all
